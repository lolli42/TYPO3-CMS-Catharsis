[client]
socket = /dev/shm/typo3ramdisk/mysqld-testing.sock

[mysqld_safe]
socket = /dev/shm/typo3ramdisk/mysqld-testing.sock

[mysqld]
user = travis
pid-file = /dev/shm/typo3ramdisk/mysqld-testing.pid
socket = /dev/shm/typo3ramdisk/mysqld-testing.sock
datadir = /dev/shm/typo3ramdisk
tmpdir = /tmp
bind-address = 127.0.0.1
port = 3307
log_error = /dev/shm/typo3ramdisk/error.log
skip-external-locking

character-set-server=utf8
collation_server=utf8_unicode_ci

innodb_file_per_table

innodb_log_file_size = 1048576

